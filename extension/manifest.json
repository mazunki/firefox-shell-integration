{
    "manifest_version": 2,
    "name": "Firefunki",
    "version": "1.0",
    "description": "Extension to list windows and tabs",
    "permissions": ["nativeMessaging"],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon-16.png",
            "48": "icons/icon-48.png",
            "128": "icons/icon-128.png"
        }
    },
    "manifest_version": 2,
	"browser_specific_settings": {
		"gecko": {
			"id": "tech.mazunki.ff_integration@mozilla.org",
			"strict_min_version": "42.0"
		}
	}
}

